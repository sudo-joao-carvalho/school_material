package com.sample;

import java.util.Objects;

public class Divisao {

	private String tipo;
	private boolean temIntruso;
	

	public Divisao(String tipo, Boolean temIntruso) {
		super();
		this.tipo = tipo;
		this.temIntruso = temIntruso;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
	
	public boolean isTemIntruso() {
		return temIntruso;
	}

	public void setTemIntruso(boolean temIntruso) {
		this.temIntruso = temIntruso;
	}

	@Override
	public int hashCode() {
		return Objects.hash(tipo);
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Divisao other = (Divisao) obj;
		return Objects.equals(tipo, other.tipo);
	}
	
}
